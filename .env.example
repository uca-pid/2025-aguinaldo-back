# Database Configuration
# Provided automatically by Render when using managed PostgreSQL
DB_HOST=your-db-host
DB_PORT=5432
DB_NAME=medibook
DB_USERNAME=your-db-username
DB_PASSWORD=your-db-password

# Server Configuration
# Provided automatically by Render
PORT=10000

# CORS Configuration
# Update this with your actual frontend URL after deployment
CORS_ALLOWED_ORIGINS=https://your-frontend-app.onrender.com

# Admin Credentials (for development/testing only)
# These will be generated automatically by Render
ADMIN_USERNAME=admin
ADMIN_PASSWORD=secure-password

# MailerSend Configuration
MAILERSEND_ENABLED=true
MAILERSEND_API_TOKEN=your_mailersend_api_token_here
MAILERSEND_FROM_EMAIL=noreply@yourdomain.com
MAILERSEND_FROM_NAME=MediBook

# Supabase Storage S3 Configuration
SUPABASE_S3_ENDPOINT=https://your-project.storage.supabase.co/storage/v1/s3
SUPABASE_S3_REGION=us-east-2
SUPABASE_S3_ACCESS_KEY=your_access_key_here
SUPABASE_S3_SECRET_KEY=your_secret_key_here