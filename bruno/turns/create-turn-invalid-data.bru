meta {
  name: Create Turn - Invalid Data
  type: http
  seq: 27
}

post {
  url: {{baseUrl}}/api/turns
  body: json
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:json {
  {
    "doctorId": "invalid-uuid",
