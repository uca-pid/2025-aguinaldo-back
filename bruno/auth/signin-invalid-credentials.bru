meta {
  name: Sign In - Invalid Credentials
  type: http
  seq: 9
}

post {
  url: {{baseUrl}}/api/auth/signin
  body: json
  auth: none
}

body:json {
  {
    "email": "nonexistent@example.com",
    "password": "wrongpassword"
  }
}

assert {
  res.status: eq 401
}

tests {
  test("Should reject invalid credentials", function() {
    expect(res.getStatus()).to.equal(401);
  });
}