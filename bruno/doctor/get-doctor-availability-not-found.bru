meta {
  name: Get Doctor Availability - Not Found
  type: http
  seq: 3
}

get {
  url: {{host}}/api/doctors/{{doctorId}}/availability
  body: none
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

headers {
  Content-Type: application/json
}

vars:pre-request {
  doctorId: 99999
}

assert {
  res.status: eq 404
}

tests {
  test("Should return 404 when doctor not found", function() {
    expect(res.getStatus()).to.equal(404);
  });
}